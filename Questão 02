#include <stdio.h>
#include <stdlib.h>

int main(void) {
// Declaração de variáveis
int i=3, j=5;
int *p, *q;
int *aux;
//Relações
p = &i;
q = &j;
//Imprimindo as questões
    //a
    p = &i;
    printf("\n Letra a) %p", p );
    //b
    int x = *q - *p;
    printf("\n Letra b) %i ", x); 
    //c
    printf("\n Letra c) %i ", **&p);
    //d
    int y = (3 - ( *p/ *q) + 7); 
    printf("\n Letra d) %i ", y);

return 0;
}







